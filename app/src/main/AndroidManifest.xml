<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maisIdade">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light.NoActionBar">
        <activity android:name=".activity.EditarArquivosActivity"></activity>
        <activity android:name=".activity.PdfActivity" />
        <activity
            android:name=".activity.ListVideosActivity"
            android:label="@string/title_activity_list_videos"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".activity.VideoActivity"
            android:label="@string/title_activity_video"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".activity.AddVideoActivity"
            android:label="@string/title_activity_add_video"
            android:theme="@style/NoActionBar" />
        <activity android:name=".activity.FormCadUserActivity" />
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
    </application>

</manifest>